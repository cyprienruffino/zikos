{
  "name": "zikos",
  "version": "0.1.0",
  "description": "AI Music Teacher POC",
  "private": true,
  "scripts": {
    "build": "tsc",
    "build:watch": "tsc --watch",
    "lint": "eslint 'frontend/src/**/*.ts' || true",
    "lint:fix": "eslint --fix 'frontend/src/**/*.ts' || true",
    "format": "prettier --write 'frontend/**/*.{ts,html,css}'",
    "format:check": "prettier --check 'frontend/**/*.{ts,html,css}'",
    "type-check": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "@vitest/coverage-v8": "^2.1.9",
    "@vitest/ui": "^2.1.8",
    "eslint": "^8.57.0",
    "jsdom": "^25.0.1",
    "prettier": "^3.2.5",
    "typescript": "^5.3.3",
    "vitest": "^2.1.8"
  }
}
