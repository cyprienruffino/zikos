[run]
source = src/zikos
omit =
    */tests/*
    */test_*.py
    */__pycache__/*
    */.*
    */_version.py

[report]
exclude_lines =
    pragma: no cover
    def __repr__
    raise AssertionError
    raise NotImplementedError
    if __name__ == .__main__.:
    if TYPE_CHECKING:
    @abstractmethod

[html]
directory = htmlcov

